    "mocha": "^3.5.3",
    "nsp": "^2.8.1",
    "nyc": "^11.3.0",
    "sinon": "^2.4.1",
    "standard-version": "^4.0.0",
    "webpack": "^2.5.1",
    "yazl": "^2.4.2"
  }
}

{
  "name": "tf-codebuild-sns-slack",
  "version": "1.0.0",
  "description": "a node.js lambda function that forwards codebuild events to slack via SNS",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist/* && webpack",
    "coverage": "nyc --reporter=lcov npm test",
    "lint": "eslint ./lib",
    "release": "standard-version",
    "sec": "nsp check --output summary",
    "test": "mocha --recursive test/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cludden/tf-codebuild-sns-slack.git"
  },
  "keywords": [],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/cludden/tf-codebuild-sns-slack/issues"
  },
  "homepage": "https://github.com/cludden/tf-codebuild-sns-slack#readme",
  "dependencies": {
    "app-container": "^1.0.0",
    "aws-sdk": "^2.149.0",
    "axios": "^0.17.0",
    "bluebird": "https://registry.npmjs.org/bluebird/-/bluebird-3.5.1.tgz",
    "bunyan": "^1.8.12",
    "lodash.attempt": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.0",
    "source-map-support": "https://registry.npmjs.org/source-map-support/-/source-map-support-0.4.18.tgz"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "temp-directory": "./coverage/.nyc_output"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "https://registry.npmjs.org/babel-register/-/babel-register-6.26.0.tgz",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "faker": "^4.1.0",
